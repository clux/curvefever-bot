{
  "name": "curvefever-bot",
  "description": "A curvefever signup and match making bot for your chat channel",
  "author": "Eirik Albrigtsen <sszynrae@gmail.com>",
  "version": "0.1.0",
  "stability": "unstable",
  "repository": {
    "type": "git",
    "url": "clux/curvefever-bot"
  },
  "keywords": [
    "game",
    "signup",
    "scraper",
    "team",
    "generator",
    "bot"
  ],
  "main": "curve-gu.js",
  "scripts": {
    "start": "node irc.js",
    "test": "$(npm bin)/nodeunit --reporter=verbose test/*.js",
    "coveralls": "$(npm bin)/jscoverage bot && CURVEFEVER_BOT_COV=1 $(npm bin)/nodeunit --reporter=lcov test | $(npm bin)/coveralls"
  },
  "dependencies": {
    "curvefever-stats": "~0.1.0",
    "gu": "~0.3.0",
    "confortable": "~0.2.1"
  },
  "optionalDependencies": {
    "irc-stream": "~0.0.5"
  },
  "devDependencies": {
    "nodeunit": "~0.9.0",
    "jscoverage": "~0.5.4",
    "coveralls": "~2.11.1"
  },
  "bugs": {
    "url": "http://github.com/clux/curvefever-bot/issues"
  },
  "license": "MIT"
}
